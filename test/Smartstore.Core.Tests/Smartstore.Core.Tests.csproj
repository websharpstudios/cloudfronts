<Project Sdk="Microsoft.NET.Sdk">

	<Import Project="..\..\src\Smartstore.Build\Smartstore.Common.props" />
	
	<PropertyGroup>
		<OutputType>Library</OutputType>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Platform\DataExchange\Files\testdata.csv" />
